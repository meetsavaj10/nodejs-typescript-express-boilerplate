{
  "name": "pcb-click-api",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "start": "nodemon src/app.server.ts",
    "start:dev": "nodemon --ext js,ts,json,env --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/app.server.ts",
    "prepare": "husky install",
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rm -rf tsconfig.build.tsbuildinfo && rm -rf ./dist",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "test:coverage": "npm run test -- --coverage",
    "test:ci": "npm run test -- --colors --coverage --ci",
    "lint": "eslint --ext .ts,.js .",
    "format": "prettier \"./**\" --write --ignore-unknown",
    "format:check": "prettier \"./**\" --ignore-unknown --check"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "body-parser": "^1.20.1",
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.4",
    "mqtt": "^4.3.7",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "redis": "^4.5.0",
    "request": "^2.88.2",
    "sequelize": "^6.25.7",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@jest/globals": "^29.3.1",
    "@jest/types": "^29.3.1",
    "@types/cors": "^2.8.12",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.2",
    "i": "^0.3.7",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "npm": "^9.1.2",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9"
  },
  "engine": {
    "node": "18.12.0"
  }
}